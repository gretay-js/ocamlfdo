(executable
 (name ocamlfdo)
 (libraries
        owee
        core
        csvfields
        compiler-libs.common
        compiler-libs.bytecomp
        compiler-libs.optcomp
        parsexp_io
        shexp.process
        ocamlcfg)
 (preprocess (pps ppx_jane ppx_csv_conv ppx_variants_conv)))

(install
 (section bin)
 (files (ocamlfdo.exe as ocamlfdo)))

(alias (name DEFAULT) (deps ocamlfdo.ml.pp))
